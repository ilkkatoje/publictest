<?xml version="1.0"?>
 
<!DOCTYPE suppressions PUBLIC
     "-//Checkstyle//DTD SuppressionFilter Configuration 1.0//EN"
     "https://checkstyle.org/dtds/suppressions_1_0.dtd">
 
<suppressions>
  <suppress checks=".*" files=".*generated.*"/>
  <suppress checks="FinalParameters" files=".*"/>
  <suppress checks="HideUtilityClassConstructor" files=".*"/>
  <suppress checks="JavadocPackage" files=".*"/>
  <suppress checks="JavadocVariable" files=".*"/>
  <suppress checks="MethodLength" files=".*Route.*.java"/>
  <suppress checks="MissingJavadocMethod" files="(Application.java|CamelConfiguration.java)"/>
  <suppress checks="RegexpSingleline" files=".java"/>
</suppressions>
